{
  "name": "ngrx-chat-course",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "install:fresh": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm shrinkwrap && npm run start",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint --format=stylish",
    "lint:fix": "ng lint --fix",
    "check": "ng build && ng build --prod && ng lint && npm run test:single",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-training:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build --prod && ng run angular-training:server:production",
    "prerender": "ng run angular-training:prerender"
  },
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@ngrx/store": "~10.1.1",
    "lodash": "~4.17.20",
    "normalize.css": "~8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "~2.0.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "~4.14.161",
    "@types/node": "~12.12.67",
    "codelyzer": "~6.0.0",
    "husky": "~4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "~0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  }
}
